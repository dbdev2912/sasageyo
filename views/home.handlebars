<link rel="stylesheet" href="/css/home/master.css">
<div class="container main-container">
    <div class="width-60">
        {{#each post}}
        <div class="_post">
            <div class="_post-header" style="position: relative">
                <div class="head-div owner-img">
                    <img src="{{postOwner.avatar}}" alt="">
                </div>
                <div class="head-div owner-name" style="z-index: 50">
                    <span class="head-post-token" href="/u/{{postOwner.user_token}}">{{postOwner.user_token}}</span>
                </div>
                <div class="head-div switch">
                    <span class="switch-notation">...</span>
                </div>
                <div class="owner-infor">
                    <div style="background: rgba(255,255,255, .9);">
                        <div class="container profile" style="margin-top: 0">
                            <div class="mini-container profile-img">
                                <img class="circle-image" src="{{postOwner.avatar}}" alt="">
                            </div>
                            <div class="profile-content">
                                <div class="profile-line">
                                    <span class="token" id="{{postOwner.user_id}}">{{postOwner.user_token}}</span>
                                </div>
                                <div class="profile-line">
                                    <p class="profile-name">{{postOwner.name}}</p>
                                </div>
                            </div>
                        </div>
                        <div class="profile-content" style="margin-top: unset;">
                            <div class="profile-line">
                                <div class="profile-link-container">
                                    <a class="link" href="{{postOwner.website}}">{{postOwner.website}}</a>
                                </div>
                            </div>
                        </div>
                        <div class="profile-content" style="width: 100%; margin-top: 5px; padding-bottom: 15px;">
                            <div class="profile-line">
                                <div class="profile-link-container">
                                    <a class="link"><span class="post-count">14</span> Bài viết</a>
                                </div>
                                <div class="profile-link-container">
                                    <a class="link" href="#"><span class="followed-count">0</span> Theo dõi</a>
                                </div>
                                <div class="profile-link-container">
                                    <a class="link" href="#">Đang theo dõi <span class="following-count">0</span> người dùng</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="owner-utils">
                    <span class="bridge owner-util-bridge"></span>
                    <div class="owner-util">
                        <span class="owner-util-label to-post-page" url="/p/{{post_id}}">Đi đến trang bài viết</span>
                    </div>
                    <div class="owner-util">
                        <span class="owner-util-label">Sao chép liên kết</span>
                    </div>
                    <div class="owner-util">
                        <span class="owner-util-label">Nhúng</span>
                    </div>
                </div>
            </div>
            <div class="_post-body">
                <!-- <div class="owner-utils" style="z-index: 49"> -->
            {{#if url}}
                <button class="next">^</button>
                <button class="prev">^</button>
            {{/if}}
                <div class="imgs-container">
                    <div class="_post-img" role="head" position="0">
                        <img class="post-img" src="{{start_url}}" alt="">
                    </div>
                    {{#each url}}
                    <div class="_post-img" >
                        <img class="post-img" src="{{this}}" alt="">
                    </div>
                    {{/each}}
                </div>
            </div>
            <div class="_post-utils">
                <div class="util like" liked="1" owner="{{postOwner.user_id}}" post_id={{post_id}}>
                    <svg aria-label="Bỏ thích" fill="#ed4956" height="24" viewBox="0 0 48 48" width="24"><path d="M34.6 3.1c-4.5 0-7.9 1.8-10.6 5.6-2.7-3.7-6.1-5.5-10.6-5.5C6 3.1 0 9.6 0 17.6c0 7.3 5.4 12 10.6 16.5.6.5 1.3 1.1 1.9 1.7l2.3 2c4.4 3.9 6.6 5.9 7.6 6.5.5.3 1.1.5 1.6.5s1.1-.2 1.6-.5c1-.6 2.8-2.2 7.8-6.8l2-1.8c.7-.6 1.3-1.2 2-1.7C42.7 29.6 48 25 48 17.6c0-8-6-14.5-13.4-14.5z"></path></svg>
                </div>
                <div class="util like" liked="0" owner="{{postOwner.user_id}}" post_id={{post_id}}>
                    <svg aria-label="Thích" fill="#262626" height="24" viewBox="0 0 48 48" width="24">
                        <path d="M34.6 6.1c5.7 0 10.4 5.2 10.4 11.5 0 6.8-5.9 11-11.5 16S25 41.3 24 41.9c-1.1-.7-4.7-4-9.5-8.3-5.7-5-11.5-9.2-11.5-16C3 11.3 7.7 6.1 13.4 6.1c4.2 0 6.5 2 8.1 4.3 1.9 2.6 2.2 3.9 2.5 3.9.3 0 .6-1.3 2.5-3.9 1.6-2.3 3.9-4.3 8.1-4.3m0-3c-4.5 0-7.9 1.8-10.6 5.6-2.7-3.7-6.1-5.5-10.6-5.5C6 3.1 0 9.6 0 17.6c0 7.3 5.4 12 10.6 16.5.6.5 1.3 1.1 1.9 1.7l2.3 2c4.4 3.9 6.6 5.9 7.6 6.5.5.3 1.1.5 1.6.5.6 0 1.1-.2 1.6-.5 1-.6 2.8-2.2 7.8-6.8l2-1.8c.7-.6 1.3-1.2 2-1.7C42.7 29.6 48 25 48 17.6c0-8-6-14.5-13.4-14.5z"></path>
                    </svg>
                </div>
                <div class="util comment">
                    <svg fill="#262626" height="24" viewBox="0 0 48 48" width="24">
                        <path clip-rule="evenodd" d="M47.5 46.1l-2.8-11c1.8-3.3 2.8-7.1 2.8-11.1C47.5 11 37 .5 24 .5S.5 11 .5 24 11 47.5 24 47.5c4 0 7.8-1 11.1-2.8l11 2.8c.8.2 1.6-.6 1.4-1.4zm-3-22.1c0 4-1 7-2.6 10-.2.4-.3.9-.2 1.4l2.1 8.4-8.3-2.1c-.5-.1-1-.1-1.4.2-1.8 1-5.2 2.6-10 2.6-11.4 0-20.6-9.2-20.6-20.5S12.7 3.5 24 3.5 44.5 12.7 44.5 24z" fill-rule="evenodd"></path>
                    </svg>
                </div>
                <div class="util share">
                    <svg fill="#262626" height="24" viewBox="0 0 48 48" width="24">
                        <path d="M47.8 3.8c-.3-.5-.8-.8-1.3-.8h-45C.9 3.1.3 3.5.1 4S0 5.2.4 5.7l15.9 15.6 5.5 22.6c.1.6.6 1 1.2 1.1h.2c.5 0 1-.3 1.3-.7l23.2-39c.4-.4.4-1 .1-1.5zM5.2 6.1h35.5L18 18.7 5.2 6.1zm18.7 33.6l-4.4-18.4L42.4 8.6 23.9 39.7z"></path>
                    </svg>
                </div>
                <div class="util saved">
                    <svg  fill="#262626" height="24" viewBox="0 0 48 48" width="24">
                        <path d="M43.5 48c-.4 0-.8-.2-1.1-.4L24 29 5.6 47.6c-.4.4-1.1.6-1.6.3-.6-.2-1-.8-1-1.4v-45C3 .7 3.7 0 4.5 0h39c.8 0 1.5.7 1.5 1.5v45c0 .6-.4 1.2-.9 1.4-.2.1-.4.1-.6.1zM24 26c.8 0 1.6.3 2.2.9l15.8 16V3H6v39.9l15.8-16c.6-.6 1.4-.9 2.2-.9z"></path>
                    </svg>
                </div>
            </div>
            <div class="_post-content">
                <span class="total-like"><span class="likes"></span> lượt thích</span>
                {{#if content}}
                <p class="content"><span class="owner-ident">{{postOwner.user_token}} </span> {{content}}</p>
                <span class="total-cmts">Tất cả <span class="cmts"></span> bình luận</span>
                {{/if}}
            </div>
            <div class="_post-comments saperate" position="0">
                <!-- <p class="content"><span class="owner-ident">minupython</span> From minu with love</p> -->
                <p style="text-align: center">
                    <button class="psedou-btn more-comments" style="display: none">Xem thêm 20 bình luận</button>
                </p>
            </div>
            <div class="make-comment">
                <input type="text" owner="{{postOwner.user_id}}" post_id="{{post_id}}" role="comment" placeholder="Thêm bình luận ..">
                <button class="psedou-btn">Đăng</button>
            </div>
        </div>
        {{/each}}
        <div class="_posts-footer">

        </div>
    </div>
    <div class="width-40 fixed">
        <div class="suggest">
            <div class="self">
                <div class="self-img">
                    <img src="{{profile.avatar}}" alt="">
                </div>
                <div class="self-name">
                    <span id="{{profile.user_id}}">{{profile.user_token}}</span>
                </div>
                <div class="psedou-btn-container">
                    <a href="/profile" class="psedou-btn">Chuyển</a>
                </div>
            </div>
            <div class="suggest-div">
                <div class="left">
                    <span>Gợi ý cho bạn</span>
                </div>
                <div class="right">
                    <span>Xem tất cả</span>
                </div>
            </div>
            <div class="suggest-users" id="suggest-users">
                <!-- <div class="user">
                    <div class="user-img">
                        <img src="/img/avatar/minu1.jpg" alt="">
                    </div>
                    <div class="user-name">
                        <span>minu1.porn</span>
                        <p>Gợi ý cho bạn</p>
                    </div>
                    <div class="psedou-btn-container">
                        <button class="psedou-btn">Theo dõi</button>
                    </div>
                </div> -->
            </div>
        </div>
    </div>
</div>


<script src="/js/home/main.js"></script>
<script src="/js/home/slide.js"></script>
<script src="/js/home/owner-infor.js"></script>
